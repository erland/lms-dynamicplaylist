<?xml version="1.0" encoding="utf-8"?>
<custombrowse>
	<requireplugins>DynamicPlayList,CustomScan</requireplugins>
	<mix>
		<mixname>Dynamic Playlists</mixname>
		<mixcategory>mixedtag.+</mixcategory>
		<mixtype>mode</mixtype>
		<mixchecktype>function</mixchecktype>
		<mixcheckdata>Plugins::DynamicPlayList::Plugin::mixable</mixcheckdata>
		<mixdata>PLUGIN.DynamicPlayList.Mixer|dynamicplaylist_parameter_1.id={itemid}|dynamicplaylist_parameter_1.name={itemname}|playlisttype={itemtype}|flatlist=1|extrapopmode=1</mixdata>
		<mixurl>plugins/DynamicPlayList/dynamicplaylist_list.html?playlisttype={itemtype}&amp;flatlist=1&amp;dynamicplaylist_parameter_1={itemid}</mixurl>
		<mixjive>dynamicplaylist mixjive {itemtype}:{itemid}</mixjive>
		<miximage>html/images/dynamicplaylist_mixbutton.gif</miximage>
	</mix>
</custombrowse>
